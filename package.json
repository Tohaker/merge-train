{
  "name": "mergetrainslackbot",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prebuild:zip": "mkdirp --mode=0700 build",
    "zip:slackbot": "jszip-cli -c slackbotzip.json",
    "zip:githubapp": "jszip-cli -c githubappzip.json",
    "build:zip": "npm run zip:slackbot && npm run zip:githubapp",
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "func start",
    "test": "jest --watch",
    "test:ci": "jest"
  },
  "dependencies": {
    "@azure/cosmos": "^3.9.3",
    "@azure/functions": "^1.2.2",
    "@slack/bolt": "^3.1.1",
    "node-fetch": "^2.6.1",
    "query-string": "^6.13.8",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@ffflorian/jszip-cli": "^3.1.0",
    "@types/jest": "^26.0.19",
    "@types/node-fetch": "^2.5.8",
    "azure-functions-core-tools": "^3.0.0",
    "jest": "^26.6.3",
    "mkdirp": "^1.0.4",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1"
  }
}
